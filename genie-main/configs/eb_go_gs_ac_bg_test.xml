				<param index="2">2.7242320E-05</param>
				<param index="1">4488.812</param>
			</paramArray>
		</model>
		<model name="goldsteinseaice">
			<param name="npstp">501</param>
			<param name="ianav">501</param>
			<param name="itstp">501</param>
			<param name="iwstp">500</param>
