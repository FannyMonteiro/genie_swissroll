ia_emissconstb=1.278144073486328e+02


# Surface fluxes, namelist INIFLUX.
# blvad: min=1, max=5
# fluxfactor: min=1, max=5
# enhancestable: min=0.05, max=0.8
# enhancelim: min=2, max=6
# blrh: min=80, max=120
